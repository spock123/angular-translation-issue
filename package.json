{
  "name": "translate",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve --open --configuration=en",
    "dev:da": "ng serve  --configuration=da --open",
    "dev:es": "ng serve  --configuration=es --open",
    "dev:it": "ng serve  --configuration=it --open",
    "clean": "rimraf dist",
    "start": "npm run dev",
    "build:dev-code": "cross-env NODE_ENV=development ng build --configuration=dev",
    "build:dev": "npm-run-all build:dev-code postbuild",
    "build:test-code": "cross-env NODE_ENV=test ng build --configuration=test",
    "build:test": "npm-run-all build:test-code postbuild",
    "build:production-code": "cross-env NODE_ENV=production ng build --configuration=production",
    "build:production": "npm-run-all build:production-code postbuild",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format": "npx prettier 'src/**/*.{js,jsx,ts,tsx,html,css,scss}' --write",
    "format:check": "prettier --config .prettierrc --check \"src/**/*.{ts,css,html}\"",
    "version": "node ./scripts/version.js",
    "postbuild": "shx cp ./src/root-index.html ./dist/corporate/browser/index.html; ",
    "i18n-merge": "ts-node -P ./scripts/i18n-merge/tsconfig.json ./scripts/i18n-merge/i18n-merge.ts",
    "t": "ng extract-i18n --format=json --out-file tmp/extract.json --progress && npm run i18n-merge",
    "serve:ssr:corporate": "node dist/corporate/server/server.mjs"
  },
  "engines": {
    "node": "20"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/cdk": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/material": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/platform-server": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@angular/service-worker": "^19.0.0",
    "@angular/ssr": "^19.0.2",
    "ajv": "^8.11.2",
    "compression": "^1.7.4",
    "cookieconsent": "^3.1.1",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "rimraf": "^4.1.2",
    "rxjs": "~7.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.1",
    "@angular-eslint/builder": "19.0.2",
    "@angular-eslint/eslint-plugin": "19.0.2",
    "@angular-eslint/eslint-plugin-template": "19.0.2",
    "@angular-eslint/schematics": "19.0.2",
    "@angular-eslint/template-parser": "19.0.2",
    "@angular/cli": "^19.0.1",
    "@angular/compiler-cli": "^19.0.0",
    "@angular/localize": "^19.0.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.14.8",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs-extra": "^9.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "shx": "^0.3.4",
    "stylelint": "^15.2.0",
    "ts-node": "~8.3.0",
    "typescript": "~5.6.3"
  }
}
